<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Subjects Data</title>
  </head>
  <body>
    <h1>Subjects Data</h1>
    <table border="1">
      <thead>
        <th>Subject Id</th>
        <th>Subject Name</th>
        <th>Teachers</th>
      </thead>
      <% data.forEach(d=>{ %>
      <tbody>
        <td><%= d.id %></td>
        <td><%= d.subject_name %></td>
        <td><ol>
          <% d.teachers.forEach(dT=>{ %>
          <li><%= `${dT.first_name} ${dT.last_name}` %></li>
          <% }) %>
        </ol></td>
      </tbody>
      <% }) %>
    </table>
  </body>
</html>
